{
  "expo": {
    "name": "presence",
    "slug": "presence",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon_light.png",
    "scheme": "presence",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/images/appicon_dark.png",
        "light": "./assets/images/appicon_light.png"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "icon": "./assets/images/appicon_light.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appicon_light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffe0a3",
          "dark": {
            "backgroundColor": "#ffe0a3"
          }
        }
      ],
      "expo-asset",
      "expo-audio",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
